$(".users").html("<%= escape_javascript(render partial: 'users', locals: {users: @users, title: 'Create user'}) %>")
<%- if @user.errors.any?%>
  $('#newuser').modal('show');
<%- else%>
  $('#newUser').on('hidden', function(){
    $(this).data('modal', null);
  });
  $('#newUser').modal('hide')
<%- end%>